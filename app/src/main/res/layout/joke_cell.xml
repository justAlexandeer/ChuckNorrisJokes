    <TextView
        android:id="@+id/jokeCell_textView"
        android:layout_margin="8dp"
        android:textSize="17sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</com.google.android.material.card.MaterialCardView>